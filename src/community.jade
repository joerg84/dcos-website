extends ../layouts/base-whitefooter.jade

block head
  title Community | DC/OS
  meta(property='og:locale', content='en_US')
  meta(property='og:type', content='article')
  meta(property='og:title', content='Community | DC/OS')
  meta(property='og:image', content='https://dcos.io/assets/images/social-img.png')
  meta(property='og:description', content='Join the DC/OS community and connect with users, contributors, partners, and supporters on GitHub, Twitter, #slack or our mailing list.')
  meta(property='og:url', content='https://dcos.io/')
  meta(property='og:site_name', content='DC/OS')
  meta(name='twitter:card', content='summary')
  meta(name='twitter:site', content='@dcos')
  meta(name='twitter:title', content='Community | DC/OS')
  meta(name='twitter:card', content='summary_large_image')
  meta(name='twitter:image', content='https://dcos.io/assets/images/social-img.png')
  meta(name='twitter:description', content='Join the DC/OS community and connect with users, contributors, partners, and supporters on GitHub, Twitter, #slack or our mailing list.')

block content

  .container.container--dark-background
    +header

    .container__content.community-hero
      h1 Join the DC/OS Community
      p.hero-subtitle Connect with our community of users and browse the latest DC/OS news.

      hr.hero-divider

      ul.container__content--hero-list
        li.hero-list__item
          img.hero-list__item__image(src="/assets/images/icons/community/github.svg")
          .hero-list__item__copy
            h5.hero-list__item__copy__title GitHub
            p.hero-list__item__copy__paragraph Are you interested in helping us make DC/OS even better? Letâ€™s work together! Check out our source code on GitHub.
            a(href='https://github.com/dcos').cta.cta--text View repositories &rarr;

        li.hero-list__item
          img.hero-list__item__image(src="/assets/images/icons/community/slack.svg")
          .hero-list__item__copy
            h5.hero-list__item__copy__title Slack
            p.hero-list__item__copy__paragraph Have any questions? Our Slack channel is the best place to get help. Just send us a request to automatically receive your invitation.
            a(href='http://chat.dcos.io').cta.cta--text Join chat &rarr;

        li.hero-list__item
          img.hero-list__item__image(src="/assets/images/icons/community/envelope.svg")
          .hero-list__item__copy
            h5.hero-list__item__copy__title Mailing List
            p.hero-list__item__copy__paragraph Want to stay in the loop and connect with other community members? Our public mailing list has all the latest updates. Join the discussion.
            a(href='https://groups.google.com/a/dcos.io/d/forum/users').cta.cta--text Join users@dcos.io &rarr;

  .container.container--grey-background.community-container
    .container__content
      h2 No Upcoming Events

      pre #{locals.events}


  .container.events-container
    .container__content
      .events-carousel
        .events-carousel-btn.events-carousel-btn--next
          img(src='/assets/images/icons/chevron-dark-right.svg')

        .events-carousel-btn.events-carousel-btn--prev
          img(src='/assets/images/icons/chevron-dark-left.svg')

        .card-container
          each event in locals.events
            a(href!=event.url target='_blank').card
              .card-header(style='background-image: url(#{event.image})')
                .card-content
                  .card-header__date
                    p #{event.day}
                    p #{event.month.toUpperCase()}
              .card-content.card-copy
                h3.card-title  #{event.title}
                .cta.cta--text More info &rarr;

    .container__content.event-cta
      a(href="mailto:events@dcos.io").cta.cta--text Want to host your own DC/OS event? &rarr;

